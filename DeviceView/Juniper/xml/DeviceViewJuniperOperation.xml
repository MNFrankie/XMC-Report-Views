<report>
	<reportId>DeviceViewJuniperOperation</reportId>
	<title>Device View Juniper Operation</title>
	<category>Device</category>
	<showInTree>false</showInTree>
	<iconCls>table-icon</iconCls>
	<deviceViewTab>
		<viewTabTitle>Operation</viewTabTitle>
		<viewOrder>1202</viewOrder>
		<families>
			<family>Juniper EX</family>
		</families>
	</deviceViewTab>
	<panel>
		<layout>fit</layout>
		<flex>1</flex>
		<showTitle>false</showTitle>
		<autoScroll>true</autoScroll>
		<items>
			<panel>
				<title>CPU and Memory</title>
				<ui>extr-bordered-panel</ui>
				<bodyBorder>true</bodyBorder>
				<items>
				<flexviewgrid>
						<hideToolbar>true</hideToolbar>
						<viewId>Junper_CPU_Memory</viewId>
					</flexviewgrid>
				</items>
			</panel>
			<splitter>
				<collapsible>true</collapsible>
				<showMiniCollapse>true</showMiniCollapse>
			</splitter>
			<panel>
				<title>Redundancy</title>
				<ui>extr-bordered-panel</ui>
				<bodyBorder>true</bodyBorder>
				<items>
					<flexviewgrid>
						<hideToolbar>true</hideToolbar>
						<viewId>Juniper_Redundancy</viewId>
					</flexviewgrid>
				</items>
			</panel>
			<splitter>
				<collapsible>true</collapsible>
				<showMiniCollapse>true</showMiniCollapse>
			</splitter>
			<panel>
				<title>LEDs</title>
				<ui>extr-bordered-panel</ui>
				<bodyBorder>true</bodyBorder>
				<items>
					<flexviewgrid>
						<hideToolbar>true</hideToolbar>
						<viewId>Juniper_LEDs</viewId>
					</flexviewgrid>
				</items>
			</panel>
		</items>
	</panel>
</report>
            