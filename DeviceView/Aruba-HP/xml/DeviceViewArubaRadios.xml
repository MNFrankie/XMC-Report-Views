				<flexviewgrid>
						<hideToolbar>true</hideToolbar>
						<viewId>wslsxWlanRadioEntry</viewId>
					</flexviewgrid>
				</items>
			</panel>
			<splitter>
				<collapsible>true</collapsible>
				<showMiniCollapse>true</showMiniCollapse>
			</splitter>
			<panel>
				<title>BSSIDs</title>
				<ui>extr-bordered-panel</ui>
				<bodyBorder>true</bodyBorder>
				<items>
					<flexviewgrid>
						<hideToolbar>true</hideToolbar>
						<viewId>wlsxWlanAPBssidEntry</viewId>
					</flexviewgrid>
