<report>
	<reportId>DeviceViewCiscoHWStatus</reportId>
	<title>Device View Cisco HW Status</title>
	<category>Device</category>
	<showInTree>false</showInTree>
	<iconCls>table-icon</iconCls>
	<deviceViewTab>
		<viewTabTitle>Power and Fan</viewTabTitle>
		<viewOrder>1009</viewOrder>
		<families>
			<family>Cisco</family>
			<family>Catalyst</family>
			<family>Cisco7200</family>
			<family>CiscoASR</family>
			<family>CiscoRouter</family>
			<family>CiscoNX</family>
		</families>
	</deviceViewTab>
	<panel>
		<layout>fit</layout>
		<flex>1</flex>
		<showTitle>false</showTitle>
		<autoScroll>true</autoScroll>
		<items>
			<panel>
				<title>Power Supply</title>
				<ui>extr-bordered-panel</ui>
				<bodyBorder>true</bodyBorder>
				<items>
				<flexviewgrid>
						<hideToolbar>true</hideToolbar>
						<viewId>Cisco_PowerSupply</viewId>
					</flexviewgrid>
				</items>
			</panel>
			<splitter>
				<collapsible>true</collapsible>
				<showMiniCollapse>true</showMiniCollapse>
			</splitter>
			<panel>
				<title>FAN Status</title>
				<ui>extr-bordered-panel</ui>
				<bodyBorder>true</bodyBorder>
				<items>
					<flexviewgrid>
						<hideToolbar>true</hideToolbar>
						<viewId>Cisco_Fan</viewId>
					</flexviewgrid>
				</items>
			</panel>
		</items>
	</panel>
</report>
            